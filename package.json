{
  "dependencies": {
    "marked": "^14.1.3",
    "pinia": "^2.0.28",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6"
  },
  "description": "Collection of Notebooks for GitHub pages",
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.9",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vueuse/core": "^11.1.0",
    "autoprefixer": "^10.4.13",
    "tailwindcss": "^3.2.4",
    "unplugin-auto-import": "^0.18.3",
    "vite": "^4.0.0",
    "vite-svg-loader": "^5.1.0"
  },
  "name": "notes",
  "prettier": {
    "arrowParens": "avoid",
    "bracketSpacing": true,
    "endOfLine": "lf",
    "printWidth": 100,
    "proseWrap": "preserve",
    "quoteProps": "as-needed",
    "requirePragma": false,
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false,
    "vueIndentScriptAndStyle": false
  },
  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "fmt": "prettier --write src && prettier --write vite.config.js",
    "preview": "vite preview"
  },
  "version": "0.1.0"
}
